#!/usr/bin/env node

"use strict";

var nodeVersion = process.versions.node;
var segments = nodeVersion.split(".");

if (segments[0] < 8) {
  console.log(
    "Error: You are running Node " + nodeVersion + ". Node 8+ is required."
  );
  process.exit(1);
}

require("../src/cli")(process.argv.slice(2));
