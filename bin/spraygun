#!/usr/bin/env node

"use strict";

var nodeVersion = process.versions.node;
var segments = nodeVersion.split(".");

if (segments[0] < 10) {
  console.log(
    "Error: You are running Node " +
      nodeVersion +
      ". Spraygun requires Node 10 or later."
  );
  process.exit(1);
}

require("../src/cli")(process.argv.slice(2));
