    "test:coverage": "jest --coverage"
  },
  "lint-staged": {
    "src/**/*.js": "eslint"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "shelljs": "^0.8.2",
    "yargs-parser": "^20.2.9"
  },
  "devDependencies": {
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "jest-junit": "^13.0.0",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1"
  },
  "engines": {
    "node": ">=14"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "src"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
